@model E_Shop.Models.Admin.StockModel

@{
    ViewData["Title"] = "Stock Details";
}

<h2>Stock Details</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">Stock ID</dt>
        <dd class="col-sm-10">@Model.StockId</dd>

        <dt class="col-sm-2">Product ID</dt>
        <dd class="col-sm-10">@Model.ProductId</dd>

        <dt class="col-sm-2">Store ID</dt>
        <dd class="col-sm-10">@Model.StoreId</dd>

        <dt class="col-sm-2">Quantity</dt>
        <dd class="col-sm-10">@Model.StockQuantity</dd>

        <dt class="col-sm-2">Stock Type ID</dt>
        <dd class="col-sm-10">@Model.StockTypeId</dd>

        <dt class="col-sm-2">Last Updated</dt>
        <dd class="col-sm-10">@Model.LastUpdated</dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.StockId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
